<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ShowDetilseHome">
<ImageView
    android:layout_width="match_parent"
    android:layout_height="@dimen/_230sdp"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    android:src="@color/cardview_dark_background"
    android:id="@+id/image"/>
    <de.hdodenhof.circleimageview.CircleImageView
        android:layout_width="@dimen/_50sdp"
        android:layout_height="@dimen/_50sdp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/image"
        android:layout_marginTop="@dimen/_210sdp"
        android:layout_marginStart="@dimen/_20sdp"
        android:src="@drawable/logodetilse"
        android:id="@+id/logodetilse"/>
<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    app:layout_constraintStart_toStartOf="@id/logodetilse"

    android:text=""

    app:layout_constraintTop_toBottomOf="@id/logodetilse"
    android:layout_marginTop="@dimen/_5sdp"
    android:layout_marginEnd="@dimen/_5sdp"
    android:textSize="@dimen/_15sdp"
    android:textColor="#181D3D"
    android:fontFamily="@font/droidkufi_bold"
    android:id="@+id/detilseDescrption"/>
        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="@dimen/_10sdp"
            android:drawableStart="@drawable/location"
            app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="@dimen/_10sdp"
            android:drawablePadding="@dimen/_5sdp"
            android:text=""
            app:layout_constraintTop_toBottomOf="@id/detilseDescrption"
            android:layout_marginTop="@dimen/_15sdp"

            android:textSize="@dimen/_12sdp"
            android:textColor="#181D3D"

            android:id="@+id/location"/>
<View
    android:layout_width="match_parent"
    android:layout_height="@dimen/_1sdp"
    app:layout_constraintTop_toBottomOf="@id/location"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    android:layout_marginTop="@dimen/_10sdp"
    android:background="#A7D7FC"
    android:layout_marginStart="@dimen/_8sdp"
    android:layout_marginEnd="@dimen/_8sdp"
    android:id="@+id/view"/>
    <LinearLayout
        android:layout_marginStart="@dimen/_10sdp"
        android:layout_marginEnd="@dimen/_10sdp"
        android:gravity="center"
        android:id="@+id/l1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/view"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="@dimen/_5sdp"
        android:orientation="horizontal">

        <TextView
            android:drawableTop="@drawable/size"
            android:layout_width="wrap_content"
            android:gravity="center"
            android:fontFamily="@font/droidkufi_bold"
            android:textSize="@dimen/_12sdp"
            android:drawablePadding="@dimen/_2sdp"
            android:layout_height="wrap_content"
            android:text=""
            android:textColor="#181D3D"

            android:id="@+id/textsize"/>
        <TextView
            android:layout_marginStart="@dimen/_50sdp"
            android:drawableTop="@drawable/roomtulat"
            android:layout_width="wrap_content"

            android:gravity="center"
            android:fontFamily="@font/droidkufi_bold"
            android:textSize="@dimen/_12sdp"
            android:drawablePadding="@dimen/_2sdp"
            android:layout_height="wrap_content"
            android:text=""
            android:textColor="#181D3D"
            android:id="@+id/textroomtoulte"/>
        <TextView
            android:layout_marginStart="@dimen/_50sdp"
            android:drawableTop="@drawable/room"

            android:gravity="center"
            android:fontFamily="@font/droidkufi_bold"
            android:textSize="@dimen/_12sdp"
            android:drawablePadding="@dimen/_2sdp"
            android:textColor="#181D3D"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:id="@+id/room"/>


    </LinearLayout>

        <LinearLayout
            android:id="@+id/l2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_5sdp"
            android:gravity="center"
            android:orientation="horizontal"
            app:layout_constraintBottom_toTopOf="@+id/loginone"

            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/l1">

            <TextView
                android:id="@+id/textland"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableTop="@drawable/land"
                android:drawablePadding="@dimen/_2sdp"
                android:fontFamily="@font/droidkufi_bold"
                android:gravity="center"
                android:text=""
                android:textColor="#181D3D"

                android:textSize="@dimen/_12sdp" />

            <TextView
                android:id="@+id/textwoter"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:layout_marginStart="@dimen/_50sdp"
                android:drawableTop="@drawable/woter"
                android:drawablePadding="@dimen/_2sdp"
                android:fontFamily="@font/droidkufi_bold"
                android:gravity="center"
                android:text=""
                android:textColor="#181D3D"
                android:textSize="@dimen/_12sdp" />

            <TextView
                android:id="@+id/wifi"
                android:layout_width="wrap_content"

                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_50sdp"
                android:drawableTop="@drawable/wifi"
                android:drawablePadding="@dimen/_2sdp"
                android:fontFamily="@font/droidkufi_bold"
                android:gravity="center"
                android:text=""
                android:textColor="#181D3D"
                android:textSize="@dimen/_12sdp" />


        </LinearLayout>

        <com.google.android.material.button.MaterialButton
            android:layout_width="@dimen/_130sdp"
            android:layout_height="@dimen/_60sdp"
            android:padding="@dimen/_15sdp"
            android:fontFamily="@font/droidkufi_bold"
            app:layout_constraintStart_toStartOf="parent"
           app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="@dimen/_5sdp"
            app:layout_constraintEnd_toEndOf="parent"
            android:text="@string/communicate"

            android:textColor="@color/white"

            app:backgroundTint="@null"
            android:background="@drawable/itemstilysend"
            app:backgroundTintMode="add"

            android:textSize="@dimen/_15sdp"
            android:textAllCaps="false"

            android:id="@+id/loginone" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>